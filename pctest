v0x1183ed0_0 .var "nextPC", 31 0;
v0x1183fb0_0 .net "regRs", 31 0, v0x11846b0_0;  1 drivers
E_0x1169f20 .event posedge, v0x1183c00_0;
E_0x1169ca0/0 .event edge, v0x1183b10_0, v0x1183fb0_0, v0x1183cc0_0, v0x1168df0_0;
E_0x1169ca0/1 .event edge, v0x1183ed0_0;
E_0x1169ca0 .event/or E_0x1169ca0/0, E_0x1169ca0/1;
    .scope S_0x1168b80;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x1183a30_0, 0;
    %end;
    .thread T_0;
    .scope S_0x1168b80;
T_1 ;
    %wait E_0x1169ca0;
    %load/vec4 v0x1183b10_0;
    %cmpi/e 0, 0, 2;
    %jmp/0xz  T_1.0, 4;
    %load/vec4 v0x1183ed0_0;
    %assign/vec4 v0x1183df0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x1183b10_0;
    %cmpi/e 1, 0, 2;
    %jmp/0xz  T_1.2, 4;
    %load/vec4 v0x1168df0_0;
    %ix/load 4, 2, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x1183df0_0, 4, 5;
    %load/vec4 v0x1183ed0_0;
    %parti/s 4, 28, 6;
    %ix/load 4, 28, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x1183df0_0, 4, 5;
    %pushi/vec4 0, 0, 2;
    %ix/load 4, 0, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x1183df0_0, 4, 5;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x1183b10_0;
    %cmpi/e 2, 0, 2;
    %jmp/0xz  T_1.4, 4;
    %load/vec4 v0x1183cc0_0;
    %load/vec4 v0x1183ed0_0;
    %add;
    %store/vec4 v0x1183df0_0, 0, 32;
    %jmp T_1.5;
T_1.4 ;
    %load/vec4 v0x1183fb0_0;
    %assign/vec4 v0x1183df0_0, 0;
T_1.5 ;
T_1.3 ;
T_1.1 ;
    %load/vec4 v0x1183a30_0;
    %addi 4, 0, 32;
    %assign/vec4 v0x1183ed0_0, 0;
