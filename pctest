#! /usr/local/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1-107-gab6ae79)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x142a540 .scope module, "testPC" "testPC" 2 8;
 .timescale -9 -12;
v0x146cec0_0 .net "PC", 31 0, v0x146c760_0;  1 drivers
v0x146cfa0_0 .var "S", 1 0;
L_0x7fe8464cd018 .functor BUFT 1, C4<0000000000000000>, C4<0>, C4<0>, C4<0>;
v0x146d070_0 .net *"_s3", 15 0, L_0x7fe8464cd018;  1 drivers
v0x146d140_0 .var "clk", 0 0;
v0x146d210_0 .var "immediate", 15 0;
v0x146d320_0 .var "jumpaddress", 25 0;
v0x146d3e0_0 .var "regRs", 31 0;
L_0x146d4b0 .concat [ 16 16 0 0], v0x146d210_0, L_0x7fe8464cd018;
S_0x1451960 .scope module, "pc" "pc_multiplexer" 2 16, 3 7 0, S_0x142a540;
 .timescale -9 -12;
    .port_info 0 /OUTPUT 32 "PC"
    .port_info 1 /INPUT 32 "immediate"
    .port_info 2 /INPUT 26 "JumpAddress"
    .port_info 3 /INPUT 32 "regRs"
    .port_info 4 /INPUT 1 "clk"
    .port_info 5 /INPUT 2 "S"
v0x1451b80_0 .net "JumpAddress", 25 0, v0x146d320_0;  1 drivers
v0x146c760_0 .var "PC", 31 0;
v0x146c840_0 .net "S", 1 0, v0x146cfa0_0;  1 drivers
v0x146c930_0 .net "clk", 0 0, v0x146d140_0;  1 drivers
v0x146c9f0_0 .net "immediate", 31 0, L_0x146d4b0;  1 drivers
v0x146cb20_0 .var "mux_out", 31 0;
v0x146cc00_0 .var "nextPC", 31 0;
v0x146cce0_0 .net "regRs", 31 0, v0x146d3e0_0;  1 drivers
E_0x1452d00 .event posedge, v0x146c930_0;
E_0x1452a80/0 .event edge, v0x146c840_0, v0x146cce0_0, v0x146c9f0_0, v0x1451b80_0;
E_0x1452a80/1 .event edge, v0x146cc00_0;
E_0x1452a80 .event/or E_0x1452a80/0, E_0x1452a80/1;
    .scope S_0x1451960;
T_0 ;
    %wait E_0x1452a80;
    %load/vec4 v0x146c840_0;
    %cmpi/e 0, 0, 2;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v0x146cc00_0;
    %assign/vec4 v0x146cb20_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x146c840_0;
    %cmpi/e 1, 0, 2;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x1451b80_0;
    %ix/load 4, 2, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x146cb20_0, 4, 5;
    %load/vec4 v0x146cc00_0;
    %parti/s 4, 28, 6;
    %ix/load 4, 28, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x146cb20_0, 4, 5;
    %pushi/vec4 0, 0, 2;
    %ix/load 4, 0, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x146cb20_0, 4, 5;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x146c840_0;
    %cmpi/e 2, 0, 2;
    %jmp/0xz  T_0.4, 4;
    %load/vec4 v0x146c9f0_0;
    %load/vec4 v0x146cc00_0;
    %add;
    %store/vec4 v0x146cb20_0, 0, 32;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x146cce0_0;
    %assign/vec4 v0x146cb20_0, 0;
T_0.5 ;
T_0.3 ;
T_0.1 ;
    %load/vec4 v0x146c760_0;
    %addi 4, 0, 32;
    %assign/vec4 v0x146cc00_0, 0;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x1451960;
T_1 ;
    %wait E_0x1452d00;
    %load/vec4 v0x146cb20_0;
    %store/vec4 v0x146c760_0, 0, 32;
    %load/vec4 v0x146c760_0;
    %addi 4, 0, 32;
    %store/vec4 v0x146cc00_0, 0, 32;
    %jmp T_1;
    .thread T_1;
    .scope S_0x142a540;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x146d140_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x142a540;
T_3 ;
    %delay 200000, 0;
    %load/vec4 v0x146d140_0;
    %nor/r;
    %store/vec4 v0x146d140_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x142a540;
T_4 ;
    %vpi_call 2 23 "$dumpfile", "pc.vcd" {0 0 0};
    %vpi_call 2 24 "$dumpvars" {0 0 0};
    %delay 400000, 0;
    %pushi/vec4 10, 0, 16;
    %store/vec4 v0x146d210_0, 0, 16;
    %pushi/vec4 20, 0, 26;
    %store/vec4 v0x146d320_0, 0, 26;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x146d3e0_0, 0, 32;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x146cfa0_0, 0, 2;
    %delay 210000, 0;
    %load/vec4 v0x146cec0_0;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_4.0, 6;
    %vpi_call 2 28 "$display", "initialization failed" {0 0 0};
T_4.0 ;
    %delay 190000, 0;
    %pushi/vec4 10, 0, 16;
    %store/vec4 v0x146d210_0, 0, 16;
    %pushi/vec4 20, 0, 26;
    %store/vec4 v0x146d320_0, 0, 26;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x146d3e0_0, 0, 32;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x146cfa0_0, 0, 2;
    %delay 210000, 0;
    %load/vec4 v0x146cec0_0;
    %cmpi/ne 4, 0, 32;
    %jmp/0xz  T_4.2, 6;
    %vpi_call 2 34 "$display", "first increment failed" {0 0 0};
T_4.2 ;
    %delay 190000, 0;
    %pushi/vec4 10, 0, 16;
    %store/vec4 v0x146d210_0, 0, 16;
    %pushi/vec4 20, 0, 26;
    %store/vec4 v0x146d320_0, 0, 26;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x146d3e0_0, 0, 32;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x146cfa0_0, 0, 2;
    %delay 210000, 0;
    %load/vec4 v0x146cec0_0;
    %cmpi/ne 8, 0, 32;
    %jmp/0xz  T_4.4, 6;
    %vpi_call 2 40 "$display", "second increment failed" {0 0 0};
T_4.4 ;
    %delay 190000, 0;
    %pushi/vec4 10, 0, 16;
    %store/vec4 v0x146d210_0, 0, 16;
    %pushi/vec4 20, 0, 26;
    %store/vec4 v0x146d320_0, 0, 26;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x146d3e0_0, 0, 32;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x146cfa0_0, 0, 2;
    %delay 210000, 0;
    %load/vec4 v0x146cec0_0;
    %cmpi/ne 12, 0, 32;
    %jmp/0xz  T_4.6, 6;
    %vpi_call 2 46 "$display", "third increment failed" {0 0 0};
T_4.6 ;
    %delay 190000, 0;
    %pushi/vec4 10, 0, 16;
    %store/vec4 v0x146d210_0, 0, 16;
    %pushi/vec4 20, 0, 26;
    %store/vec4 v0x146d320_0, 0, 26;
    %pushi/vec4 30, 0, 32;
    %store/vec4 v0x146d3e0_0, 0, 32;
